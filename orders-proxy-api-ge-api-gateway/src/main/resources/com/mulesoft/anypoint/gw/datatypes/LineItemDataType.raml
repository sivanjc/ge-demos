#%RAML 1.0 DataType

type: object
properties:
  id:
    type: string
    required: true
  sku:
    type: string
  unit-price:
    type: number
    format: float
    minimum: 1.0
  quantity:
    type: integer
    minimum: 1
  final-price:
    type: number
    format: float
  taxable:
    type: boolean
  inv-stock-qty:
    type: integer
    required: false
  